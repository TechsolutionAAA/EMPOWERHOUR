{
  "name": "empower-hour",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write 'src/**/*.js'",
    "dev": "node_modules/.bin/ts-node-dev --respawn --notify false --transpile-only --ignore-watch node_modules -- src/index.ts",
    "start": "nodemon --watch dist dist/index.js",
    "watch": "tsc -w src/index.ts",
    "build": "tsc",
    "production:start": "cross-env NODE_ENV=production node dist/index.js",
    "prod": "bash ./scripts/production.sh"
  },
  "dependencies": {
    "backblaze-b2": "^1.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/backblaze-b2": "^1.5.1",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
